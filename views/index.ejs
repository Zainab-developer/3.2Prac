<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= "DevOps Glossary" %></h1>
    <h2><%= sub %></h2>
    <table>
      <tr>
        <th>ID#</th>
        <th>Term</th>
        <th>Description</th>
        <th>References</th>
      </tr>
      <% names.forEach((name) => { %>
      <tr>
          <td><%= name.id %></td>
          <td><%= name.term %></td>
          <td><%= name.description %></td>
          <td><%= name.reference %></td>
      </tr>
    <% }) %>
    </table>
  </body>
</html>
